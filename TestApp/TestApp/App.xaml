<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="TestApp.App">
	<Application.Resources>
		<ResourceDictionary>
            <Color x:Key="PrimaryLabelText">#1EA2DC</Color>
            <Color x:Key="BarBackground">#283860</Color>
            <Color x:Key="EditorBackground">#283860</Color>
            <Color x:Key="BorderBottom">#2CB3EC</Color>
            <Color x:Key="NavBarItem">#1EA2DC</Color>
            <Color x:Key="CellBackground">##303C60</Color>
            <Style TargetType="NavigationPage">
                <Setter Property="BarBackgroundColor" Value="{StaticResource BarBackground}" />
                <Setter Property="BarTextColor" Value="White" />
            </Style>
        </ResourceDictionary>
	</Application.Resources>
</Application>
